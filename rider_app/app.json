{
  "expo": {
    "name": "E-Boda Rider",
    "slug": "eboda-rider",
    "version": "1.0.0",
    "icon": "./assets/icon.jpg",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermissions": "Allow E-Boda to access your location"
        }
      ]
    ],
    "android": {
      "permissions": ["android.permission.ACCESS_COARSE_LOCATION", "android.permission.ACCESS_FINE_LOCATION"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAMkHHOM7rJf0GV9uUb8qSs8msmhhXrn7I"
        }
      }
    },
    "ios": {
      "supportsTabletMode": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "E-Boda needs access your location to find nearby drivers",
        "NSLocationAlwaysUsageDescription": "E-Boda needs access to your location to find nearby drivers"
      }
    }
  }
}
